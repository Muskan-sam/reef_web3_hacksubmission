#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../@open-web3/dev-config/scripts/polkadot-exec-prettier.cjs" "$@"
  ret=$?
else 
  node  "$basedir/../../../../@open-web3/dev-config/scripts/polkadot-exec-prettier.cjs" "$@"
  ret=$?
fi
exit $ret
